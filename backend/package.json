{
  "name": "@BLOGS/backend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js --exec \"node --no-warnings --loader ts-node/esm --experimental-specifier-resolution=node\" src/index.ts",
    "build-ts": "rimraf ./dist && tsc --build ./tsconfig.build.json --force",
    "build": "pnpm build-ts && pnpm fix-imports",
    "fix-imports": "ts-add-js-extension --dir=dist --verbose",
    "start": "cross-env NODE_ENV=production node -r tsconfig-paths/register dist/src/index.js",
    "test": "jest --runInBand --detectOpenHandles",
    "sentry-release": "dotenv -- sentry-cli releases new %SOURCE_VERSION% --finalize --org aleksey-cy --project backend --auth-token %SENTRY_AUTH_TOKEN%",
    "sentry-sourcemaps": "dotenv -- sentry-cli releases files %SOURCE_VERSION% upload-sourcemaps ./dist --org aleksey-cy --project backend",
    "sentry": "pnpm sentry-release && pnpm sentry-sourcemaps",
    "pmd": "prisma migrate dev",
    "pmt": "dotenv -e ./.env prisma migrate deploy",
    "pmp": "dotenv -e ./.env.production prisma migrate deploy",
    "pgc": "prisma generate",
    "types": "tsc --noEmit --project ./tsconfig.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache src/**/*.ts",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "prepare": "pnpm pgc ts-patch install -s"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@BLOGS/blogsApp": "workspace:*",
    "@BLOGS/shared": "workspace:*",
    "@prisma/client": "^6.2.1",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.12.0",
    "@trpc/server": "^10.45.2",
    "cloudinary": "^2.6.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "eslint": "9.20.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "picocolors": "^1.1.1",
    "prisma": "^6.2.1",
    "randomstring": "^1.3.1",
    "react-router-dom": "^7.2.0",
    "serialize-error": "^12.0.0",
    "superjson": "1.13.3",
    "triple-beam": "^1.4.1",
    "trpc-playground": "^1.0.4",
    "typescript": "5.7.3",
    "winston": "^3.17.0",
    "yaml": "^2.7.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@sentry/cli": "^2.43.0",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/lodash": "^4.17.14",
    "@types/node": "20.5.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/randomstring": "^1.3.0",
    "@types/triple-beam": "^1.3.5",
    "@typescript-eslint/parser": "^8.24.1",
    "env-cmd": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^3.1.9",
    "ts-add-js-extension": "^1.6.5",
    "ts-node": "^10.9.2"
  }
}
