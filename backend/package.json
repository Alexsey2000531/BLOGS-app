{
  "name": "@BLOGS/backend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node ./.env --loader ts-node/esm ./src/index.ts",
    "build-ts": "rimraf ./dist && tsc --build ./tsconfig.json",
    "build": "pnpm build-ts",
    "start": "node ./dist/index.js",
    "pmd": "prisma migrate dev",
    "pgc": "prisma generate",
    "types": "tsc --noEmit --project ./tsconfig.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache src/**/*.ts",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "prepare": "pnpm pgc"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@trpc/server": "^10.45.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "9.20.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "superjson": "1.13.3",
    "trpc-playground": "^1.0.4",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/lodash": "^4.17.14",
    "@types/node": "20.5.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint-plugin-node": "^11.1.0",
    "prisma": "^6.2.1",
    "ts-node-dev": "^2.0.0"
  }
}
